  title = {Jailbreaking {{Black Box Large Language Models}} in {{Twenty Queries}}},
  author = {Chao, Patrick and Robey, Alexander and Dobriban, Edgar and Hassani, Hamed and Pappas, George J. and Wong, Eric},
  date = {2023-10-13},
  eprint = {2310.08419},
  eprinttype = {arxiv},
